<div id='{{name}}' class='page noselect'>
  <div class='background' id="{{name}}BgContainer"></div>

  <div id="promptContainer" class='container fillAbs centeredContainer column noselect twoPartScreen' style="z-index:2;">
    <div class="type_lg title over_video">
      {{{copy.large}}}
      <div class="type_med" style="margin-top:.2rem;">
        {{{copy.small}}}
      </div>
    </div>
    <div id="{{name}}Buttons">
      <div id="{{name}}Prev" class="type_med button button_wide" ondragstart='return false;' onclick="window.events.dispatchEvent(new Event('prev'));">
          {{{copy.yes}}}
      </div>
      <div id="{{name}}Next" class="type_med button button_wide" ondragstart='return false;' onclick="window.events.dispatchEvent(new Event('next'));">
          {{{copy.no}}}
      </div>
    </div>
  </div>
</div>
