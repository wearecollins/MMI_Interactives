<div id='{{name}}' class='page noselect'>
  <div class='background colorBg'></div>

  <audio src="{{audio}}" id="vo_grab"></audio>

  <div class="container fillAbs centeredContainer column">
    <div class="copyContainer type_lg centeredContainer">
      <div class="type_lg title">
        {{{copy.large}}}
      </div>
    </div>

    <div id="{{name}}NextBtn" class="button nextButton" ondragstart='return false;' onclick="window.events.dispatchEvent(new Event('next'));">
      {{{copy.next}}}
    </div>
  </div>
</div>
