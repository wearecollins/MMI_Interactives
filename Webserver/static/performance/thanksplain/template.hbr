<div id='{{name}}' class='page noselect'>
  <div class='background fill'></div>

  <div class="container fillAbs centeredContainer column" style="z-index:2; justify-content:space-around;">
    <div class='type_lg title'>
      {{{copy}}}
    </div>
    <div id="{{name}}NextBtn" class="button" ondragstart='return false;' onclick="window.events.dispatchEvent(new Event('next'));">
      Start Over
    </div>
  </div>
</div>
