<div id='{{name}}' class='page'>
  <!-- <audio src="vo/countdown.mp3" id="snd_countdown"></audio> -->

  <audio src="{{audio}}" id="vo_{{name}}"></audio>

  <div id="videoReviewContainer" class="fillAbs centeredContainer column colorBg" syle="z-index:0">
  	<div id='{{name}}Text' class="type_lg">
      {{{copy}}}
    </div>
    <video id="videoReview" class="fillAbs" preload="none">
        <source src="" type="video/mp4">
    </video>
    <div id='{{name}}Buttons'>

	    <div id="{{name}}NextBtn" class="button" ondragstart='return false;' onclick="window.events.dispatchEvent(new Event('next'));">
        Yes
    	</div>
    	<div class='buttonSpacer type_lg'>
        &nbsp;
      </div>
	    <div id="{{name}}CancelBtn" class="button" ondragstart='return false;' onclick="window.events.dispatchEvent(new Event('special'));">
        No
    	</div>
    </div>
    <div id='{{name}}PlayContainer' class="container fillAbs centeredContainer column">
      <div class="copyContainer type_lg centeredContainer">
        <div class="type_lg title">
          &nbsp;
        </div>
      </div>

      <div id='{{name}}Play' class='button'>
        Play
      </div>
    </div>
  </div>

</div>
