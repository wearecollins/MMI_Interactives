<div id='{{name}}' class='page'>
  <div class='background'>
    <video id='{{name}}_video'>
      <source src="{{@path}}/{{video}}" />
    </video>
  </div>
  <div class='container'>
    <div class='content'>
    </div>
    <div class='buttons'>
      {{#each buttons}}
      <div class='{{type}}' 
           onclick="window.dispatchEvent(new Event('{{type}}'));">
        {{text}}
      </div>
      {{/each}}
    </div>
  </div>
</div>
