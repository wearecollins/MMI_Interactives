<div id='{{name}}' class='page'>
  <div class='background'></div>
  <div class='container'>
    <div class='content'>
      {{#each animatedText}}
        <div>{{text}}</div>
      {{/each}}
    </div>
    <div class='buttons'>
      {{#each buttons}}
      <div class='{{type}}' 
           onclick="window.events.dispatchEvent(new Event('{{type}}'));">
        {{text}}
      </div>
      {{/each}}
    </div>
  </div>
</div>
